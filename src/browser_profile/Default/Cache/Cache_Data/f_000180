    });
    if($("#bannerSlide").find("img").length > 1){

        $("#bannerSlide").hover(function () {
            $(this).find(".prev,.next").stop(true, true).fadeTo("show", 0.2)
        }, function () {
            $(this).find(".prev,.next").fadeOut()
        });

        $("#bannerSlide").slide({
            mainCell:".bd ul",
            effect:"leftLoop",
            //是否启用点击切换幻灯片，默认为滑动切换
            //trigger: "click",
            autoPlay:true
        });
    }else if($("#bannerSlide").find("img").length == 1){
        $("#bannerSlide").slide({
            mainCell:".bd ul",
                effect:"leftLoop",
                //是否启用点击切换幻灯片，默认为滑动切换
                trigger: "click",
                autoPlay:false
        });
    } else{
        $("#bannerSlide").hide();
    }
</script>

