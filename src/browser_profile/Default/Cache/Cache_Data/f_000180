        padding: 20px;
    }
    .kuzy-box .tablet-list{
        line-height: 1;
        height: 16px;
        white-space: nowrap;
        margin-bottom: 12px;
        font-size: 12px;
        color: #000000;
        overflow: hidden;
    }
    .kuzy-box .last{
        margin-bottom: 0;
    }
    .kuzy-box .tablet-list span{
        padding-right: 20px;
    }
    .kuzy-box .tablet-list a{
        color: #0099CC;
        padding-right: 18px;
    }
