<script type="text/javascript">
    window.onload = function () {
        $(".js-click-stoppropagation").click(function (t) {
            t.stopPropagation()
        });
        var t = $(".js-faculty-hidden");
        $(".js-area-ul li a").click(function () {
            if($(this).hasClass('cur')){
              return;
            }
            var index = $(this).attr("data-key");
            var curProvince = areaList[index];
            addNew(curProvince.placeId, facultyKey);
        });
        $(".js-second-areaList-ul").delegate('a', 'click', function() {
          $(this).addClass("cur");
          var placeId = $(this).data("placeid");
          addNew(String(placeId), facultyKey)
        });

        $(".js-faculty-ul li a").click(function () {
            $(this).parent().siblings().children("a").removeClass();
            $(this).addClass("cur");
            var l = $(this);
            var c = l.attr("data-key");
            if (facultyList[c].secondFacultyList != null && facultyList[c].secondFacultyList.length > 0) {
                t.html(l.text());
                $(".js-faculty-ul li a").removeAttr("href");
                var s = "";
                for (var i = 0; i < facultyList[c].secondFacultyList.length; i++) {
                        s += "<li class='faculty2-li'><a id=b" + facultyList[c].secondFacultyList[i].id + " data-facultyid=" + facultyList[c].secondFacultyList[i].id + " data-facultykey=" + facultyList[c].secondFacultyList[i].facultykey + "  >" + facultyList[c].secondFacultyList[i].secfacultyName + "</a></li>"
                }
                $(".js-faculty2-ul").html(s);
                if ($(".js-faculty2-ul").is(":hidden")) {
                    $(".js-faculty2-ul").show()
                }
            } else {
                $(".js-faculty-selected").html($(this).html());
                //facultyId = $(this).data("facultyid");
                //a(placeId, facultyId)
                facultyKey = $(this).data("facultykey");
                addNew(placeId, facultyKey)
            }
        });
        $(".js-faculty2-ul li a").live("click", function () {
            $(this).parent().siblings().children("a").removeClass();
            $(this).addClass("cur");
            $(".js-faculty-selected").html(t.text() + " " + $(this).text());
            //facultyId = $(this).data("facultyid");
            facultyKey = $(this).data("facultykey");
            addNew(placeId, facultyKey)
        });
        $(".js-doc-ul li").click(function () {
            $(this).siblings().removeClass("cur");
            $(this).addClass("cur");
            complex = $(this).data("type");
            //a(placeId, facultyId)
            addNew(placeId, facultyKey)
        });
        $(".js-area-closed").click(function () {
            $(".js-area-selected").html("全国");
            $(".js-area-ul li").children("a").removeClass("cur");
            $(".js-area-ul li").first().children("a").addClass("cur");
            placeId = "all";
            //a(placeId, facultyId)
            addNew(placeId, facultyKey)
        });
        $(".js-faculty-closed").click(function () {
            $(".js-faculty-selected").html("所有科室");
            $(".js-faculty-ul li").children("a").removeClass("cur");
            $(".js-faculty-ul li").first().children("a").addClass("cur");
            $(".js-faculty2-ul").hide();
            t.html("");
            //facultyId = 0;
            //a(placeId, facultyId)
            addNew(placeId, "");
        });
        $(".js-fam-doc-ul").on("click", ".js-fam-doc-li", function () {
            var t = $(this).data("src");
            window.open(t)
        });

        // $(".js-h-f-item").click(function () {
        //     factType = $(this).data("type");
        //     window.open("showdoctorlist?from=" + from + "&factType=" + factType)
        // });

        function a(placeId, faculty) {
            var url = "https://www.haodf.com/doctor/list";
            if (0 !== placeId && '0' !== placeId && 'all' !== placeId) {
                url=url+"-"+placeId;
            } else {
                url=url+'-all';
            }
            if (0 !== faculty && '0' !== faculty) {
                url=url+"-"+faculty;
            }
            url = url + ".html";
            window.location.href = url;
        }

        function addNew(placeId, facultyKey) 
        {/*{{{*/
            var url = "https://www.haodf.com/doctor/list";
            if (0 !== placeId && '0' !== placeId && 'all' !== placeId) {
                if (-1 != placeId.indexOf('-'))
                {
                    let placeIdArr = placeId.split('-');
                    placeId = placeIdArr[0];
                }
                url=url+"-"+placeId;
            } else {
                url=url+'-all';
            }
            if ('' !== facultyKey) {
                url=url+"-"+facultyKey;
            }
            url = url + ".html";
            window.location.href = url;
        }/*}}}*/

        var l = $(".js-f-o-ul"), c;

        function s() {
            var t = l.find("li").first().outerHeight(true);
            var a = "marginTop";
            var c = 1;
            var s = {marginTop: -t * c + "px"};
            l.animate(s, 3e3, function () {
                for (var t = 0; t < c; t++) {
                    $(".js-f-o-ul > li").first().appendTo(l)
                }
                l.css(a, "0px")
            })
        }

        l.hover(function () {
            clearInterval(c)
        }, function () {
            c = setInterval(function () {
                s()
            }, 6e3)
        }).trigger("mouseleave")
    };

    // 设置city 列表
    function setCityList(cityList){
      var cityWrapper = $(".js-second-areaList-ul");
      var s = "";
      for (var i = 0;i<cityList.length;i++) {
        var city = cityList[i];
        s += "<li class='faculty2-li'><a class=city-" + city.placeId + " data-placeid=" + city.placeId + ">" + city.placeShortName + "</a></li>"
      }
      cityWrapper.html(s);
      if (cityWrapper.is(":hidden")) {
        cityWrapper.show()
      }
    }
</script>
<script>
$(document).ready(function(){
    // 设置city 列表
    function setCityList(cityList){
      var cityWrapper = $(".js-second-areaList-ul");
      var s = "";
      for (var i = 0;i<cityList.length;i++) {
        var city = cityList[i];
        s += "<li class='faculty2-li'><a class=city-" + city.placeId + " data-placeid=" + city.placeId + ">" + city.placeShortName + "</a></li>"
      }
      cityWrapper.html(s);
      if (cityWrapper.is(":hidden")) {
        cityWrapper.show()
      }
    }
    // 设置地区默认值
    function initAreaValue(){
      for(var i=0;i<areaList.length;i++){
        var province = areaList[i];
        for(var j=0;j<province.children.length;j++){
          var city = province.children[j];
          if(city && (+city.placeId === +placeId)){
            $('.province-' + province.placeId).addClass('cur');
            setCityList(province.children);
            $('.city-' + city.placeId).addClass('cur');
            return;
          }
        }
      }
      $('.province-0').addClass('cur');
    }
    // 兼容 safari 12 版本默认样式问题
    // https://developer.apple.com/forums/thread/691608
    function hackInitCss(){
      var element = $('.area-ul .area-li .cur')[0];
      if(element){
        var disp = element.style.display;
        element.style.display = 'none';
        var trick = element.offsetHeight;
        element.style.display = disp;
      }
    }
    initAreaValue();
    hackInitCss();
    var firstFaculty = $("#a" + selectFacultyId);
    var selectedFaculty = $(".js-faculty-hidden");
    firstFaculty.parent().siblings().children("a").removeClass();
    // firstFaculty.addClass("cur");
    var i = firstFaculty.data('key');
    if (facultyList[i].secondFacultyList != null && facultyList[i].secondFacultyList.length > 0) {
        selectedFaculty.html(firstFaculty.text());
        $('.js-faculty-ul li a').removeAttr("href");
        var htmlStr = '';
        for (var j = 0; j < facultyList[i].secondFacultyList.length; j++) {
            htmlStr += "<li class='faculty2-li'><a id=b" + facultyList[i].secondFacultyList[j].id + " data-facultyid=" + facultyList[i].secondFacultyList[j].id + " data-facultykey=" + facultyList[i].secondFacultyList[j].facultykey + "  >" + facultyList[i].secondFacultyList[j].secfacultyName + "</a></li>";
        }
        $('.js-faculty2-ul').html(htmlStr);
        if ($('.js-faculty2-ul').is(":hidden")) {
            $('.js-faculty2-ul').show();
        }

        var secondFaculty = $("#b" + facultyId);
        secondFaculty.parent().siblings().children("a").removeClass();
        secondFaculty.addClass("cur");
        $('.js-faculty-selected').html(selectedFaculty.text() + " " + secondFaculty.text());
    } else {
        $('.js-faculty-selected').html(firstFaculty.html());
        if (i == 0) {
            $('.js-faculty-closed').remove();
        }
    }

    if(isScroll)
    {
        $('body,html').scrollTop($('.js-d-area').offset().top+20);
    }
})
</script>

<style>
    .c-tab {
        margin-top: 60px;
        margin-left: auto;
        margin-right: auto;
    }
</style>
<!--
!!! 使用声明 !!!
1. 引用的页面需要声明：$navigationSelected的属性，和$urlDataList中的val属性进行对应，这样页面的属性才会选中，否则不会选中
2. 因为导航头是定高样式，需要调整引入页面的 body margin-top属性
-->

