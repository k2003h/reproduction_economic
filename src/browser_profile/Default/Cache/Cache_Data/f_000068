<script>
    let jumpUrlAfterLogin = "https://passport.haodf.com/user/showlogin?forward=" + encodeURIComponent(window.location.href);
    document.querySelector('.login').setAttribute("href", jumpUrlAfterLogin);
    const hdf = {
        // 获取cookie信息
        getCookie: function (name) {
            if (typeof (name) !== 'string') return null;
            name = name.replace('[', '\\[').replace(']', '\\]');
            const results = document.cookie.match('(^|; )' + name + '=([^;]*)(;|$)');
            return results ? results[2] : null;
        }

    };
    const loginpage = {
        init: function () {
            //const isSpace = Boolean(hdf.getCookie('userinfo[id]') && parseInt(hdf.getCookie('userinfo[hostid]')) > 0 && hdf.getCookie('userinfo[hosttype]') === 'Doctor');
            spaceLoginbar.init();
        }
    };

    const loginbar = {
        init: function () {
            this.initUI();
            this.uId = hdf.getCookie('userinfo[id]');
            // 是否是登录状态
            this.isLogin = Boolean(this.uId);
            this.isLogin ? this.initLoginStatus() : this.initUnLoginStatus();
        },
        initUI: function () {
            this.jsLoginStart = document.querySelector('.js-loginstart');
            this.jsPatientLogin = document.querySelector('.js-patient-login');
        },
        initLoginStatus: function () {
            this.getServerData();
            this.jsLoginStart.style.display = 'none';
            this.jsPatientLogin.style.display = 'block';
        },
        initUnLoginStatus: function () {
            this.jsLoginStart.style.display = 'block';
            this.jsPatientLogin.style.display = 'none';
        },
        // 未读消息
        getServerData: function () {
            window.__getServerData = function (data) {
                const jsPoint = document.querySelector('.js-patient-point');
                const cnt = data.data.cnt;
                // 未读消息信封的class
                if (cnt > 0) {
                    jsPoint.style.display = 'block';
                }
            };
            const script = document.createElement('script');
            const body = document.getElementsByTagName('body');
            script.src = 'https://www.haodf.com/napi/jsonp/getLoginBarData?callback=__getServerData';
            body[0].appendChild(script);
        }


    };
    const spaceLoginbar = {
        init: function () {
            this.initUI();
            this.uId = hdf.getCookie('userinfo[id]');
            // 是否是登录状态
            this.isLogin = Boolean(this.uId);
            this.isLogin ? this.initLoginStatus() : this.initUnLoginStatus();
        },
        initUI: function () {
            this.jsLoginStart = document.querySelector('.js-loginstart');
            this.jsPatientLogin = document.querySelector('.js-doctor-login');
        },
        initLoginStatus: function () {
            this.getServerData();
            this.jsLoginStart.style.display = 'none';
            this.jsPatientLogin.style.display = 'block';
        },
        initUnLoginStatus: function () {
            this.jsLoginStart.style.display = 'block';
            this.jsPatientLogin.style.display = 'none';
        },
        // 未读消息
        getServerData: function () {
            window.__getServerData = function (data) {
                const jsPoint = document.querySelector('.js-doctor-point');
                const cnt = data.data.cnt;
                console.log('cnt--', cnt);
                // 未读消息信封的class
                if (cnt > 0) {
                    jsPoint.style.display = 'block';
                }
            };
            const script = document.createElement('script');
            const body = document.getElementsByTagName('body');
            script.src = 'https://www.haodf.com/napi/jsonp/getLoginBarData?callback=__getServerData';
            body[0].appendChild(script);
        }
    };
    loginpage.init();
</script>
